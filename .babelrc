{
  "presets": [
    [
      "@babel/env", 
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread",
     [
      "module-resolver",
      {
        "alias": {
          "@configs": "./src/configs",
          "@routes": "./src/routes",
          "@services": "./src/services",
          "@controllers": "./src/controllers",
          "@middlewares": "./src/middlewares",
          "@models": "./src/models",
          "@utils": "./src/utils"
        }
      }
    ]
  ]
}